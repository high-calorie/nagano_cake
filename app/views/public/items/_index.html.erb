<table class='table table-hover table-inverse'>
  <tbody>
    <% items.each do |item| %>
      <div class="col-md-2">

        <div class="card-image-top">
          <td><%= link_to(item.admin) do %>
          <%= f.file_field :image, accept: 'image/*' %>
          <% end %>
        </td>
        </div>

        <div class="card-body">
          <% item.name %>
          <% item.net_price %>
        </div>
        <% end %>
      </div>
  </body>
</table>