<table class='table table-hover table-inverse'>
  <tbody>
    <% items.each do |item| %>
      <div class="col-md-2">

        <div class="card-image-top">
          <td><%= link_to(item) do %>
          <%= image_tag item.image, size:"100x100" %>
          <% end %>
        </td>
        </div>

        <div class="card-body">
          <td><%= item.name %></td>
          <td><%= item.add_tax_net_price.to_s(:delimited) %>(<%= item.net_price.to_s(:delimited) %>)å††</td>
        </div>
        <% end %>
      </div>
  </body>
</table>